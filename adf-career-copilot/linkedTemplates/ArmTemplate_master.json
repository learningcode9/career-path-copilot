{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-career-copilot"
		},
		"ls_adls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls'"
		},
		"ls_adls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://stcareercc.dfs.core.windows.net/"
		},
		"trg_landing_blob_created_properties_pl_landing_to_bronze_parameters_fileUrl": {
			"type": "string",
			"defaultValue": "@triggerBody().data.url"
		},
		"trg_landing_blob_created_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/7dde6a16-56b7-452e-b264-eff27a033c50/resourceGroups/rg-career-copilot/providers/Microsoft.Storage/storageAccounts/stcareercc"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-career-copilot_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adls_accountKey": {
						"value": "[parameters('ls_adls_accountKey')]"
					},
					"ls_adls_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_properties_typeProperties_url')]"
					},
					"trg_landing_blob_created_properties_pl_landing_to_bronze_parameters_fileUrl": {
						"value": "[parameters('trg_landing_blob_created_properties_pl_landing_to_bronze_parameters_fileUrl')]"
					},
					"trg_landing_blob_created_properties_typeProperties_scope": {
						"value": "[parameters('trg_landing_blob_created_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}